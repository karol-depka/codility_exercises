import {solution} from "./solution";

describe('fut', () => {

  it(`codility example`, () => {
    expect(solution( [1, 3, 6, 4, 1, 2])).toEqual(5);
  });

  it(`codility example`, () => {
    expect(solution( [1, 2, 3])).toEqual(4);
  });


  it(`codility example`, () => {
    expect(solution( [-1, -3])).toEqual(1);
  });

  it(`1 missing`, () => {
    expect(solution( [2, 3])).toEqual(1);
  });
  it(`1 missing`, () => {
    expect(solution( [-1, 2, 3])).toEqual(1);
  });

  it(`empty`, () => {
    expect(solution( [])).toEqual(1);
  });


});
